{
  "name": "youtube-downloader",
  "productName": "Youtube Downloader",
  "version": "1.0.0",
  "author": "Clayton Industries",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "watchify -t browserify-css index.js -t babelify -o dist/js/bundle.js --debug --verbose",
    "prod": "browserify -t browserify-css index.js -t babelify -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin | uglifyjs --compress --mangle > dist/prod/bundle.js"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify-css": "^0.14.0",
    "bundle-collapser": "^1.3.0",
    "electron": "^2.0.6",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.5",
    "envify": "^4.1.0",
    "moment": "^2.24.0",
    "rc-slider": "^8.6.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "uglify-js": "^3.4.6",
    "uglifyify": "^5.0.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "babel-polyfill": "^6.26.0"
  }
}
