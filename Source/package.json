{
  "name": "youtube-downloader",
  "productName": "Youtube Downloader",
  "version": "1.0.0",
  "author": "Clayton Industries",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "watchify index.js -t babelify -o dist/js/bundle.js --debug --verbose",
    "prod": "browserify index.js -t babelify -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin | uglifyjs --compress --mangle > dist/prod/bundle.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "bundle-collapser": "^1.2.1",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1",
    "envify": "^4.1.0",
    "uglify-js": "^3.0.27",
    "uglifyify": "^4.0.3"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "electron": "github:johannesjo/electron-prebuilt",
    "material-ui": "^1.0.0-beta.3",
    "material-ui-icons": "^1.0.0-alpha.19",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
