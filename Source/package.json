{
  "name": "youtube-downloader",
  "productName": "Youtube Downloader",
  "version": "1.0.0",
  "author": "Clayton Industries",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "watchify -t browserify-css index.js -t babelify -o dist/js/bundle.js --debug --verbose",
    "prod": "browserify -t browserify-css index.js -t babelify -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin | uglifyjs --compress --mangle > dist/prod/bundle.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify-css": "^0.13.1",
    "bundle-collapser": "^1.3.0",
    "electron": "github:johannesjo/electron-prebuilt",
    "electron-packager": "^9.1.0",
    "electron-reload": "^1.2.2",
    "envify": "^4.1.0",
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.1",
    "rc-slider": "^8.4.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "uglify-js": "^3.1.9",
    "uglifyify": "^4.0.4"
  },
  "dependencies": {
    "adm-zip": "^0.4.7"
  }
}
